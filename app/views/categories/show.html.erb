<h3>Showing <%= @category.name%></h3>
<h4> All Posts in <%= @category.name%></h4>
<% @category.posts.each do |cat|%>
<div class="well">
<%= link_to cat.title, post_path(@post)%>
</div>
<% end %>
<p><%= link_to "Edit", edit_category_path(@category) %></p>
<p><%= link_to "Back", categories_path %></p>